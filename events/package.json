{
  "name": "exchange-bot-events",
  "version": "1.5.15",
  "license": "MIT",
  "main": "index.js",
  "type": "module",
  "projectId": "bkper-cc-bot",
  "functionName": "prodGen2",
  "scripts": {
    "clean": "rm -rf ./dist & rm -rf ./node_modules & wait",
    "upgrade:js": "bun update bkper-js --latest",
    "upgrade:app": "bun update bkper --latest",
    "upgrade:api": "bun update @bkper/bkper-api-types --latest",
    "prebuild": "bun clean && bun install --yarn",
    "build": "tsc && cp package.json dist/ && cp yarn.lock dist/",
    "gcp-build": "yarn install --frozen-lockfile",
    "predev": "bun run build",
    "dev": "run-p dev:*",
    "dev:compile": "tsc -w --incremental",
    "dev:webhook": "node webhook",
    "dev:nodemon": "NODE_ENV=development nodemon --watch ./dist/ --exec bun run functions:dev",
    "functions:dev": "functions-framework --source=dist/ --target=doPost --port=3003",
    "functions:enable": "gcloud services enable cloudfunctions.googleapis.com cloudbuild.googleapis.com artifactregistry.googleapis.com run.googleapis.com logging.googleapis.com --project $npm_package_projectId",
    "functions:authorize": "gcloud functions add-invoker-policy-binding $npm_package_functionName --region=us-central1 --member=serviceAccount:bkper-hrd@appspot.gserviceaccount.com --project $npm_package_projectId",
    "functions:open": "open \"https://console.cloud.google.com/functions/details/us-central1/$npm_package_functionName?organizationId=305881217394&project=$npm_package_projectId&tab=logs\"",
    "open": "yarn functions:open",
    "deploy": "bun run build && yarn --cwd dist/ deploy:dist",
    "deploy:dist": "gcloud beta functions deploy $npm_package_functionName --gen2 --region=us-central1 --no-allow-unauthenticated --runtime=nodejs22 --entry-point=doPost --trigger-http --max-instances=15 --memory=256Mi --cpu=0.083 --timeout=360s --project=$npm_package_projectId"
  },
  "devDependencies": {
    "@bkper/bkper-api-types": "^5.17.1",
    "@ngrok/ngrok": "^1.4.1",
    "@types/express": "^4.17.10",
    "@types/node": "^14.11.2",
    "bkper": "^3.3.3",
    "gts": "^3.0.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.0.3",
    "yarn": "^1.22.22"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^2.0.0",
    "bkper-js": "^1.14.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-http-context": "^1.2.4",
    "gaxios": "^4.1.0",
    "node-cache": "^5.1.2",
    "source-map-support": "^0.5.19"
  }
}